module SmallExample;



interface SmallExampleI {
  Unit multiplica(Int a);
  Unit enter();
}


class SmallExample() implements SmallExampleI {
      Int suma = 0;
      Int numero = 0;
Unit multiplica(Int a){
     Int i = 0;
     while(i<a){
       suma = suma+numero;
     }
}

Unit enter(){
     numero = 5;
     this.multiplica(3);
}


}

{
SmallExample s = new local SmallExampleI();
s!enter();

}

class B1() implements B {
 
Unit f(){
 	skip;
}

Unit q(){
 	skip;
}

Unit g(Fut<Unit> w){
	skip;
 	await w?;
 	skip;
}

Unit h(Fut<Unit> a,Fut<Unit>b){
    skip;
    Fut<Unit> r;
    B i = new local B1();
    r = i!g(a);
    skip;
    await r?;

}

Unit k(Fut<Unit> a,Fut<Unit>b){
    skip;
    await a?;
    skip;
    await b?;
    skip;

}

}

//main
{
	A l=new local A1();
	l!m();

}
